<div class="multiple_selection hidden">
    <span class="count"/>
    <ul>
        <li><a class="associate" href="#">{{t "actions.associate_with_another_workspace"}}</a></li>
    </ul>
</div>

{{#if objectName}}
    <div class="info">
        <div title="{{objectName}}" class="name">{{objectName}}</div>
        <div class="type">{{typeString}}</div>
    </div>

    {{#if noCredentials}}
        <div class="notice no_credentials"><span class="bottom">{{noCredentialsWarning}}</span></div>
    {{/if}}

    <div class="last_import">
        {{#if importFailed}}
            <div class="icon">
                <img src="/images/message_error_small.png" />
            </div>
        {{/if}}

        {{#if importInProgress}}
            <div class="icon">
                <img src="/images/in_progress.png" />
            </div>
            <div class="import_in_progress right">
                {{inProgressText}}
            </div>
        {{/if}}
        <div class="last_import_text {{#if importInProgress}}right{{/if}}">
            {{lastImport}}
        </div>
    </div>

    <div class="next_import">
        {{nextImport}}
    </div>


    <div class="actions">
        <ul>
            {{#if workspace}}
                {{#if activeWorkspace}}
                    <li><a class="dialog" data-dialog="NotesNew" href="#" data-entity-id="{{id}}" data-entity-type="{{entityType}}" data-display-entity-type="{{displayEntityType}}" data-workspace-id="{{workspaceId}}" data-allow-workspace-attachments="true">{{t "actions.add_note"}}</a></li>
                    <li><a class="associate" href="#">{{t "actions.associate_with_another_workspace"}}</a></li>
                    {{#if isChorusView}}
                        <li><a class="create_database_view dialog" href="#" data-dialog="CreateDatabaseView">{{t "actions.create_database_view"}}</a></li>
                        <li><a class="duplicate" data-dialog="VerifyChorusView" href="#">{{t "dataset.chorusview.duplicate"}}</a></li>
                    {{/if}}
                {{/if}}
            {{else}}
                <li><a class="dialog" data-dialog="NotesNew" href="#" data-entity-id="{{id}}" data-entity-type="{{entityType}}" data-display-entity-type="{{displayEntityType}}">{{t "actions.add_note"}}</a></li>
                <li><a class="associate" href="#">{{t "actions.associate_with_workspace"}}</a></li>
            {{/if}}

            {{#if activeWorkspace}}
                {{#if canExport}}
                    {{#if hasSandbox}}
                        <li><a class="import_now dialog" data-dialog="ImportScheduler" href="#">{{t "actions.import_now"}}</a></li>
                        {{#if hasSchedule}}
                            <li><a class="edit_schedule dialog" data-dialog="ImportScheduler" href="#">{{t "actions.edit_schedule"}}</a></li>
                        {{else}}
                            <li><a class="create_schedule dialog" data-dialog="ImportScheduler" href="#">{{t "actions.create_schedule"}}</a></li>
                        {{/if}}
                    {{else}}
                        <li><span class="import_now disabled">{{t "actions.import_now"}}</span></li>
                        {{#if hasSchedule}}
                            <li><span class="edit_schedule disabled">{{t "actions.edit_schedule"}}</span></li>
                        {{else}}
                            <li><span class="create_schedule disabled">{{t "actions.create_schedule"}}</span></li>
                        {{/if}}
                    {{/if}}
                {{/if}}
            {{/if}}

            {{#unless noCredentials}}
                {{#if listMode}}
                    <li><a class="tabular_data_preview" href="#">{{t "actions.tabular_data_preview"}}</a></li>
                {{/if}}
            {{/unless}}

             {{#if canAnalyze}}
                <li><a class="analyze" href="#">{{t "dataset.actions.analyze"}}</a></li>
             {{/if}}

            {{#if activeWorkspace}}
                {{#if isDeleteable}}<li><a href="#" class="alert" data-alert="DatasetDelete" data-key-prefix="{{deleteMsgKey}}">{{t deleteTextKey}}</a></li>{{/if}}
            {{/if}}
        </ul>
    </div>

    <div class="tab_control"></div>
{{/if}}
