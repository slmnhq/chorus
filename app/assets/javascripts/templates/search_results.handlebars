{{#if hasResults}}
    <div class="this_workspace selectable"></div>
    <div class="workfile_list selectable"></div>
    <div class="attachment_list selectable"></div>
    <div class="hdfs_list selectable"></div>
    <div class="dataset_list selectable"></div>
    <div class="instance_list selectable"></div>
    <div class="workspace_list selectable"></div>
    <div class="user_list selectable"></div>
{{else}}
    <div class="sorry">
        <div class="header">{{t "search.no_results.header"}}</div>
        <ul>
            <li>{{t "search.no_results.check_spelling"}}</li>
            <li>{{t "search.no_results.try_wildcards"}}</li>
            {{#if isConstrained}}
                <li><a href="{{expandHref}}">{{t "search.no_results.expand"}}</a></li>
            {{/if}}
        </ul>
    </div>
{{/if}}
